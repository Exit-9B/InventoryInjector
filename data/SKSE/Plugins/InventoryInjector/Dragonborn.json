{
  "$schema": "https://raw.githubusercontent.com/Exit-9B/InventoryInjector/main/docs/InventoryInjector.schema.json",
  "rules": [
    {
      "match": {
        "formType": "MiscItem",
        "formId": {
          "anyOf": [
            "Dragonborn.esm|1CAC0",
            "Dragonborn.esm|1CAC1"
          ]
        }
      },
      "assign": {
        "subType": "dragonclaw"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "formId": {
          "anyOf": [
            "Dragonborn.esm|2145A",
            "Dragonborn.esm|3166F",
            "Dragonborn.esm|31670",
            "Dragonborn.esm|31671",
            "Dragonborn.esm|31672"
          ]
        }
      },
      "assign": {
        "subType": "Gem"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "formId": "Dragonborn.esm|247F9"
      },
      "assign": {
        "iconLabel": "misc_trollskull"
      }
    },
    {
      "match": {
        "formType": "Weapon",
        "formId": {
          "anyOf": [
            "Dragonborn.esm|179C9",
            "Dragonborn.esm|206F2",
            "Dragonborn.esm|398E6"
          ]
        }
      },
      "assign": {
        "subType": "pickaxe"
      }
    }
  ]
}
